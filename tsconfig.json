{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "downlevelIteration": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "module": "commonjs",
        "target": "es5",
        "inlineSourceMap": true,
        "outDir": "rules",
        "lib": [
            "es2015",
            "es2016.array.include",
            "dom"
        ]
    },
    "include": ["src/*.ts"]
}